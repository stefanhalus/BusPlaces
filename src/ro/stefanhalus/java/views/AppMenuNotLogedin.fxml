<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="menuBar" accessibleHelp="Se deschide bara de meniuri" accessibleText="Bară de meniuri"
         xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="ro.stefanhalus.java.views.AppMenuNotLogedinController">
    <menus>
        <Menu mnemonicParsing="false" text="Fișier">
            <items>
                <MenuItem mnemonicParsing="false" onAction="#goToCompany" text="Opțiuni"/>
                <MenuItem fx:id="menuFileExit" mnemonicParsing="false" onAction="#ExitApplication" text="Ieșire"/>
            </items>
        </Menu>
    </menus>
</MenuBar>
